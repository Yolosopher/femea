doctype html
html(lang="en")
    head
        link(rel="stylesheet", href="assets/css/swiper-bundle.min.css")
        include ../layouts/_head
    body(class=pageinfo.title=== 'Home' ? 'homebody' : '')
        include ../layouts/_header
        main.homemain.container
            //- .homemain__herocontents
            section.homemain__herocontent
                .homemain__herocontent__cover
                .homemain__herocontent__logobox
                    include ../components/logo
                .homemain__herocontent__articles.swiper-container
                    ul.homemain__herocontent__articles__ul.swiper-wrapper
                        each name, index in articleNames
                            li.swiper-slide
                                h3
                                    a(href="#")= name
                        each name, index in articleNames
                            li.swiper-slide
                                h3
                                    a(href="#")= name
            section.homemain__contactinfo
                .homemain__contactinfo__content
                    h4 გამოგვყევი
                    ul.homemain__contactinfo__socials
                        li.fb
                            a(href="#", target='_blank')
                        li.ig
                            a(href="#", target='_blank')
                        li.yt
                            a(href="#", target='_blank')
                    h4 safe you
                    p გადმოწერე აპლიკაცია, იყავი უსაფრთხოდ!
                    .homemain__contactinfo__downloads
                        a.googleplay(href='#', target='_blank')
                            span Android
                            img.mob(src="assets/img/downloads/googleplay.png", alt="") 
                            img.desk(src="assets/img/downloads/code.png", alt="") 
                        a.appstore(href='#', target='_blank') 
                            span IOS
                            img.mob(src="assets/img/downloads/appstore.png", alt="") 
                            img.desk(src="assets/img/downloads/code.png", alt="") 
            section.homemain__newestarticles
                .innercontainer
                    .homemain__newestarticles__header.sectionheader
                        h2 უახლესი სტატიები >
                        .homemain__newestarticles__navigation
                            .homemain__newestarticles__navigation__left.real
                                include ../components/sliderarrowright
                            .homemain__newestarticles__navigation__right.real
                                include ../components/sliderarrowright
                .homemain__newestarticles__slider.innercontainer
                    .homemain__newestarticles__swiper.swiper-container.real
                        .homemain__newestarticles__wrapper.swiper-wrapper
                            each article, index in articles
                                article.homemain__newestarticles__slide.swiper-slide
                                    a.homemain__newestarticles__slide__imgbox(href='#')
                                        img(src=article.image, alt=article.title)
                                    .homemain__newestarticles__slide__textbox
                                        h3
                                            a(href="#")= article.title
                                        p= article.description
                                    a.homemain__newestarticles__slide__category(href="#", style=`--categorycolor: ${article.color};`)= article.category
                            each article, index in articles
                                article.homemain__newestarticles__slide.swiper-slide
                                    a.homemain__newestarticles__slide__imgbox(href='#')
                                        img(src=article.image, alt=article.title)
                                    .homemain__newestarticles__slide__textbox
                                        h3
                                            a(href="#")= article.title
                                        p= article.description
                                    a.homemain__newestarticles__slide__category(href="#", style=`--categorycolor: ${article.color};`)= article.category
                            each article, index in articles
                                article.homemain__newestarticles__slide.swiper-slide
                                    a.homemain__newestarticles__slide__imgbox(href='#')
                                        img(src=article.image, alt=article.title)
                                    .homemain__newestarticles__slide__textbox
                                        h3
                                            a(href="#")= article.title
                                        p= article.description
                                    a.homemain__newestarticles__slide__category(href="#", style=`--categorycolor: ${article.color};`)= article.category
            
            section.homemain__populararticles
                .innercontainer
                    .homemain__populararticles__header.sectionheader
